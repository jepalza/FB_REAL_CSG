Declare Function GetConceptualFaceEx StdCall Alias "GetConceptualFaceEx"(ByVal OwlInstances As OwlInstance ,ByVal index As INT64_N ,ByVal startIndexTriangles As INT64_P ,ByVal noIndicesTriangles As INT64_P ,ByVal startIndexLines As INT64_P ,ByVal noIndicesLines As INT64_P ,ByVal startIndexPoints As INT64_P ,ByVal noIndicesPoints As INT64_P ,ByVal startIndexFacePolygons As INT64_P ,ByVal noIndicesFacePolygons As INT64_P ,ByVal startIndexConceptualFacePolygons As INT64_P ,ByVal noIndicesConceptualFacePolygons As INT64_P) As ConceptualFace 












Static Shared As ULongInt flagbit0 = 1           ' 2^^0    0000.0000..0000.0001
Static Shared As ULongInt flagbit1 = 2           ' 2^^1    0000.0000..0000.0010
Static Shared As ULongInt flagbit2 = 4           ' 2^^2    0000.0000..0000.0100
Static Shared As ULongInt flagbit3 = 8           ' 2^^3    0000.0000..0000.1000

Static Shared As ULongInt flagbit4 = 16          ' 2^^4    0000.0000..0001.0000
Static Shared As ULongInt flagbit5 = 32          ' 2^^5    0000.0000..0010.0000
Static Shared As ULongInt flagbit6 = 64          ' 2^^6    0000.0000..0100.0000
Static Shared As ULongInt flagbit7 = 128         ' 2^^7    0000.0000..1000.0000

Static Shared As ULongInt flagbit8 = 256         ' 2^^8    0000.0001..0000.0000
Static Shared As ULongInt flagbit9 = 512         ' 2^^9    0000.0010..0000.0000
Static Shared As ULongInt flagbit10 = 1024       ' 2^^10   0000.0100..0000.0000
Static Shared As ULongInt flagbit11 = 2048       ' 2^^11   0000.1000..0000.0000

Static Shared As ULongInt flagbit12 = 4096       ' 2^^12   0001.0000..0000.0000
Static Shared As ULongInt flagbit13 = 8192       ' 2^^13   0010.0000..0000.0000
Static Shared As ULongInt flagbit14 = 16384      ' 2^^14   0100.0000..0000.0000
Static Shared As ULongInt flagbit15 = 32768      ' 2^^15   1000.0000..0000.0000

static Shared As ULongInt flagbit16 = 65536      ' 2^^16   0000.0000..0000.0001  0000.0000..0000.0000
static Shared As ULongInt flagbit17 = 131072     ' 2^^17   0000.0000..0000.0010  0000.0000..0000.0000
static Shared As ULongInt flagbit18 = 262144     ' 2^^18   0000.0000..0000.0100  0000.0000..0000.0000
static Shared As ULongInt flagbit19 = 524288     ' 2^^19   0000.0000..0000.1000  0000.0000..0000.0000

static Shared As ULongInt flagbit20 = 1048576    ' 2^^20   0000.0000..0001.0000  0000.0000..0000.0000
static Shared As ULongInt flagbit21 = 2097152    ' 2^^21   0000.0000..0010.0000  0000.0000..0000.0000
static Shared As ULongInt flagbit22 = 4194304    ' 2^^22   0000.0000..0100.0000  0000.0000..0000.0000
static Shared As ULongInt flagbit23 = 8388608    ' 2^^23   0000.0000..1000.0000  0000.0000..0000.0000

static Shared As ULongInt flagbit24 = 16777216   ' 2^^24   0000.0001..0000.0000  0000.0000..0000.0000
static Shared As ULongInt flagbit25 = 33554432   ' 2^^25   0000.0010..0000.0000  0000.0000..0000.0000
static Shared As ULongInt flagbit26 = 67108864   ' 2^^26   0000.0100..0000.0000  0000.0000..0000.0000
static Shared As ULongInt flagbit27 = 134217728  ' 2^^27   0000.1000..0000.0000  0000.0000..0000.0000

static Shared As ULongInt flagbit28 = 268435456  ' 2^^28   0001.0000..0000.0000  0000.0000..0000.0000
static Shared As ULongInt flagbit29 = 536870912  ' 2^^29   0010.0000..0000.0000  0000.0000..0000.0000
static Shared As ULongInt flagbit30 = 1073741824 ' 2^^30   0100.0000..0000.0000  0000.0000..0000.0000
static Shared As ULongInt flagbit31 = 2147483648 ' 2^^31   1000.0000..0000.0000  0000.0000..0000.0000

Sub ConfiguraSalidaDatos(model As LongInt)

	Dim As LongInt	setting = 0, mask = 0 

	'mask = GetFormat(0, 0) ' si lo pongo, no funciona la operacion booleana
	
	mask += 1 * flagbit2         '    PRECISION (32/64 bit)
	mask += 1 * flagbit3         '	 INDEX ARRAY (32/64 bit)
	
	mask += 1 * flagbit5         '    NORMALS
	
	mask += 1 * flagbit8         '    TRIANGLES
	mask += 1 * flagbit9         '    LINES
	mask += 1 * flagbit10        '    POINTS
	
	mask += 1 * flagbit12        '    WIREFRAME
	mask += 1 * flagbit13        '    WIREFRAME (CONCEPTUAL FACES)	
	
	'  Initializing the mask with all possible options
	setting += 1 * flagbit2        '    SINGLE / DOUBLE PRECISION (float / double)
	setting += 1 * flagbit3        '    32 / 63 BIT INDEX ARRAY (int32_t / int64_t)
	
	setting += 0 * flagbit4        '    VECTORS (x, y, z) 
	setting += 0 * flagbit5        '    NORMALS (Nx, Ny, Nz)
	setting += 0 * flagbit6        '    TEXTURE I (u1, v1)
	setting += 0 * flagbit7        '    TEXTURE II (u2, v2)
	
	setting += 1 * flagbit8        '    TRIANGLES
	setting += 1 * flagbit9        '    LINES
	setting += 0 * flagbit10       '    POINTS
	
	setting += 1 * flagbit12       '    WIREFRAME FACES
	setting += 0 * flagbit13       '    WIREFRAME CONCEPTUAL FACES
	
	setting += 0 * flagbit14       '    STRUCTURE WIREFRAME AS POLYGON / TUPLE
	setting += 0 * flagbit15       '    ADVANCED NORMALS (I.E. FOLLOWING THE SEMANTIC MEANING OF THE CONCEPT)
	
	setting += 0 * flagbit16       '    DIRECTX (COMBINATION SETTING BIT 20, 21, 22, 23)
	setting += 0 * flagbit17       '    OPENGL (COMBINATION SETTING BIT 20, 21, 22, 23)
	
	setting += 0 * flagbit18       '    EXPORT BOTH SIDES OF TRIANGLES
	
	setting += 0 * flagbit20       '    EXPERT SETTING TRIANGLE WINDING CW / ACW
	setting += 0 * flagbit21       '    EXPERT SETTING NONE / VERTEX + NORMAL Y = Z / Z = -Y
	setting += 0 * flagbit22       '    EXPERT SETTING
	setting += 0 * flagbit23       '    EXPERT SETTING
	
	setting += 0 * flagbit24       '    AMBIENT COLOR COMPONENT
	setting += 0 * flagbit25       '    DIFFUSE COLOR COMPONENT
	setting += 0 * flagbit26       '    EMISSIVE COLOR COMPONENT
	setting += 0 * flagbit27       '    SPECULAR COLOR COMPONENT
	
	setting += 0 * flagbit28       '    TEXTURE I TANGENT
	setting += 0 * flagbit29       '    TEXTURE I BINORMAL
	
	setting += 0 * flagbit30       '    TEXTURE II TANGENT (64 BIT ONLY)
	setting += 0 * flagbit31       '    TEXTURE II BINORMAL (64 BIT ONLY)

	SetFormat(model, setting, mask) 
End Sub
